{
  "expo": {
    "name": "WeatherApp",
    "slug": "WeatherApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "assets": [
      "assets/01d.png",
      "assets/01n.png",
      "assets/02d.png",
      "assets/02n.png",
      "assets/03d.png",
      "assets/03n.png",
      "assets/04d.png",
      "assets/04n.png",
      "assets/09d.png",
      "assets/09n.png",
      "assets/10d.png",
      "assets/10n.png",
      "assets/11d.png",
      "assets/11n.png",
      "assets/13d.png",
      "assets/13n.png",
      "assets/50d.png",
      "assets/50n.png"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
